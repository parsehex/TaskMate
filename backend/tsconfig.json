{
  "compilerOptions": {
    "target": "es2020",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "outDir": "../.dist/",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./**/*.ts", "../electron/main.ts"],
  "exclude": ["node_modules", "**/prompt_parts.ts"]
}
