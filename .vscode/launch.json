{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeArgs": [
        "-r",
        "dotenv/config"
      ],
      "program": "${workspaceFolder}/dist/backend/index.js",
      "outFiles": [
        "${workspaceFolder}/dist/backend/**/*.js"
      ],
      "sourceMaps": true
    }
  ]
}
