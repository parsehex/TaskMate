{
	"version": 2,
	"controller": {},
	"workers": [
		{
			"type": "router",
			"options": {
				"pythonpath": [".."]
			},
			"realms": [
				{
					"name": "realm1",
					"roles": [
						{
							"name": "anonymous",
							"permissions": [
								{
									"uri": "",
									"match": "prefix",
									"allow": {
										"call": true,
										"register": true,
										"publish": true,
										"subscribe": true
									},
									"disclose": {
										"caller": false,
										"publisher": false
									},
									"cache": true
								}
							]
						}
					]
				}
			],
			"transports": [
				{
					"type": "websocket",
					"endpoint": {
						"type": "tcp",
						"port": 8585
					},
					"url": "ws://0.0.0.0:8585/ws"
				}
			]
		}
	]
}
