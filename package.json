{
	"name": "taskmate",
	"version": "1.0.0",
	"description": "",
	"type": "module",
	"main": "electron/main.js",
	"source": [
		"frontend/index.html"
	],
	"targets": {
		"frontend": {
			"source": "frontend/index.html",
			"distDir": ".dist/frontend",
			"context": "browser"
		}
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"scripts": {
		"f:reset": "rimraf .dist/frontend",
		"f:dev": "vite",
		"f:build": "vite build",
		"b:reset": "rimraf .dist/backend",
		"b:dev": "tsc -p backend --watch",
		"b:watch": "nodemon --watch .dist/backend .dist/backend/index.js",
		"b:build": "tsc -p backend",
		"server": "node .dist/backend/index.js",
		"reset": "npm run f:reset && npm run b:reset",
		"w:build": "npm run f:build && npm run b:build",
		"start": "concurrently \"npm run f:dev\" \"npm run b:dev\"",
		"dev": "npm run start",
		"e:dev": "npm run w:build && cp package.json schema.sql ./.dist/ && electron ./.dist",
		"e:build": "npm run w:build && cp package.json pnpm-lock.yaml schema.sql ./.dist/ && node build.js",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:serve": "vitepress serve docs"
	},
