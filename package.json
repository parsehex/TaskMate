	"targets": {
		"frontend": {
			"source": "frontend/index.html",
			"distDir": "dist/frontend",
			"context": "browser"
		}
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"scripts": {
		"f:reset": "rimraf dist/frontend && rimraf .parcel-cache",
		"f:dev": "parcel watch",
		"f:build": "parcel build",
		"b:reset": "rimraf dist/backend",
		"b:dev": "tsc -p backend --watch",
		"b:watch": "nodemon -r dotenv/config --watch dist/backend dist/backend/index.js",
		"b:build": "tsc -p backend",
		"server": "node -r dotenv/config dist/backend/index.js",
		"reset": "npm run f:reset && npm run b:reset",
		"build": "npm run f:build && npm run b:build",
		"start": "concurrently \"npm run f:dev\" \"npm run b:dev\""
	},
