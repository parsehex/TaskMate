{
	"name": "chatgpt-project-prompter",
	"version": "1.0.0",
	"description": "",
	"source": [
		"frontend/index.html",
		"backend/index.ts"
	],
	"targets": {
		"frontend": {
			"source": "frontend/index.html",
			"distDir": "dist",
			"context": "browser"
		}
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"scripts": {
		"f:dev": "parcel watch",
		"f:build": "parcel build frontend/index.html",
		"b:dev": "tsc -p backend --watch",
		"b:watch": "nodemon -r dotenv/config dist/backend/index.js",
		"server": "node -r dotenv/config dist/backend/index.js",
		"b:build": "tsc -p backend",
		"build": "npm run f:build && npm run b:build",
		"start": "concurrently \"npm run f:dev\" \"npm run b:dev\""
	},
	"dependencies": {
		"@monaco-editor/react": "^4.5.0",
		"aws-sdk": "^2.1368.0",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"express-static-gzip": "^2.1.7",
		"file-type": "^18.3.0",
		"fs-extra": "^11.1.1",
		"glob": "^10.2.2",
		"gpt-tokenizer": "^1.0.4",
		"monaco-editor": "^0.38.0",
		"promised-sqlite3": "^2.0.1",
		"react": "^18.2.0",
		"react-dnd": "^16.0.1",
		"react-dnd-html5-backend": "^16.0.1",
		"react-dom": "^18.2.0",
		"sqlite3": "^5.1.6",
		"tslib": "^2.5.0"
	},
	"devDependencies": {
		"@parcel/transformer-sass": "^2.8.3",
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.17",
		"@types/fs-extra": "^11.0.1",
		"@types/node": "^18.16.3",
		"@types/react": "^18.2.0",
		"@types/react-dom": "^18.2.1",
		"@types/sqlite3": "^3.1.8",
		"assert": "^2.0.0",
		"browserify-zlib": "^0.2.0",
		"buffer": "^5.7.1",
		"concurrently": "^8.0.1",
		"crypto-browserify": "^3.12.0",
		"events": "^3.3.0",
		"nodemon": "^2.0.22",
		"os-browserify": "^0.3.0",
		"parcel": "^2.8.3",
		"path-browserify": "^1.0.1",
		"process": "^0.11.10",
		"punycode": "^1.4.1",
		"querystring-es3": "^0.2.1",
		"stream-browserify": "^3.0.0",
		"stream-http": "^3.2.0",
		"typescript": "^5.0.4",
		"url": "^0.11.0",
		"util": "^0.12.5"
	}
}
